{
  "name": "custom-chat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "server": "node server/server.js",
    "dev": "concurrently \"npm run server\" \"npx vite serve\"",
    "dev:host": "concurrently \"npm run server\" \"npx vite serve --host 0.0.0.0\"",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "dotenv": "^16.4.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "openai": "^4.27.1",
    "phet-lib": "~0.0.4"
  },
  "devDependencies": {
    "@types/jquery": "~3.5.13",
    "@types/lodash": "~4.14.172",
    "@types/p2": "~0.7.39",
    "@types/pako": "~2.0.0",
    "@types/qunit": "~2.19.6",
    "@types/three": "~0.137.0",
    "@typescript-eslint/eslint-plugin": "~6.18.1",
    "@typescript-eslint/parser": "~6.18.1",
    "@typescript-eslint/utils": "~6.18.1",
    "@webgpu/types": "~0.1.40",
    "concurrently": "^8.2.2",
    "eslint": "~8.28.0",
    "typescript": "~5.3.3",
    "vite": "^5.1.0"
  },
  "eslintConfig": {
